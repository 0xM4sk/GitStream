{
  "name": "gitstream-user-auth",
  "version": "1.0.0",
  "description": "GitStream User Authentication System with AWS Cognito",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "type-check": "tsc --noEmit",
    "deploy:dev": "npm run build && aws s3 sync dist/ s3://gitstream-auth-dev --delete",
    "deploy:staging": "npm run build && aws s3 sync dist/ s3://gitstream-auth-staging --delete",
    "deploy:prod": "npm run build && aws s3 sync dist/ s3://gitstream-auth-prod --delete"
  },
  "dependencies": {
    "@aws-amplify/auth": "^6.0.14",
    "@aws-amplify/core": "^6.0.14",
    "amazon-cognito-identity-js": "^6.3.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "axios": "^1.6.2",
    "js-cookie": "^3.0.5",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/js-cookie": "^3.0.6",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "typescript": "^5.2.2",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@tailwindcss/forms": "^0.5.7",
    "jsdom": "^23.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xM4sk/GitStream.git"
  },
  "keywords": [
    "authentication",
    "aws-cognito",
    "oauth",
    "react",
    "typescript",
    "email-password"
  ],
  "author": "GitStream Team",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}